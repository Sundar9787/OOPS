class student:
    def __init__(self,name,age):
        self.name = name
        self.__age=age
        
    def get_age(self):
        return self.__age
        
    def set_age(self,age):
        if age > 0:
            self.__age = age 
            
s = student("Sundar",20)
print(s.name)
print(s.get_age())

s.set_age(22)
print(s.get_age())
