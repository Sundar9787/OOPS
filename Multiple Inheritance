class Father:
    def show_father(self):
        print("I am the father")

class Mother:
    def show_mother(self):
        print("I am the Mother")
        
class Child(Father,Mother):
    def show_child(self):
        print("I am the child")
        
c = Child()
c.show_father()
c.show_mother()
c.show_child()
